l=[1,2,3,4,5,6,7,8,9,2]
m=len(l)//2
l1=[]
for i in range(0,m):
    l1.append(l[i])
l2=[]
for j in range(m,len(l)):
    l2.append(l[j])
    a=l2[::-1]
for i in a:
    l1.append(i)
print(l1)    
  //first half of the list items will be printed as usual second half of the list items will bw printed in reverse order  
